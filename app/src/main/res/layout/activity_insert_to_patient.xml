<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <EditText
        android:id="@+id/EditText_pin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="식별 번호" />

    <EditText
        android:id="@+id/EditText_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="이름" />

    <EditText
        android:id="@+id/EditText_age"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="나이" />

    <EditText
        android:id="@+id/EditText_telephone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="전화번호" />

    <EditText
        android:id="@+id/EditText_Protector_Name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="보호자 이름" />

    <EditText
        android:id="@+id/EditText_Protector_Phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="보호자 전화번호" />


    <TextView
        android:id="@+id/SendJson_Result_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/httpTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="등록하기" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

</LinearLayout>
